{
    "name": "hlint",
    "displayName": "hlint",
    "description": "Lint Haskell with HLint",
    "version": "0.5.1",
    "homepage": "https://marketplace.visualstudio.com/items?itemName=lunaryorn.hlint",
    "bugs": "https://github.com/lunaryorn/vscode-hlint/issues",
    "icon": "haskell-logo.png",
    "author": {
        "name": "Sebastian Wiesner",
        "email": "sebastian@swsnr.de",
        "url": "http://www.lunaryorn.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lunaryorn/vscode-hlint.git"
    },
    "license": "Apache-2.0",
    "publisher": "lunaryorn",
    "engines": {
        "vscode": "^1.12.0"
    },
    "galleryBanner": {
        "color": "#19111f",
        "theme": "dark"
    },
    "categories": [
        "Linters",
        "Languages",
        "Formatters"
    ],
    "keywords": [
        "haskell",
        "hlint",
        "refactor"
    ],
    "badges": [
        {
            "url": "https://travis-ci.org/lunaryorn/vscode-hlint.svg?branch=master",
            "href": "https://travis-ci.org/lunaryorn/vscode-hlint",
            "description": "Build Status"
        }
    ],
    "activationEvents": [
        "onLanguage:haskell"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "haskell",
                "aliases": [
                    "Haskell",
                    "haskell"
                ],
                "extensions": [
                    ".hs",
                    ".lhs"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "rxjs": "^6.0.0",
        "semver": "^5.5.0",
        "tmp": "^0.0.33"
    },
    "devDependencies": {
        "@types/node": "^9.6.6",
        "@types/semver": "^5.5.0",
        "@types/tmp": "^0.0.33",
        "tslint": "^5.2.0",
        "tslint-immutable": "^4.0.1",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.0.3",
        "vsce": "^1.37.6",
        "vscode": "^1.0.0"
    }
}
